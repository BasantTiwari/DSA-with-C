#include<stdio.h>
int main()
{
    int arr[10],i,n,temp;
    printf("Enter th elements \n");
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    for(i=0;i<n;i++)
    {
        printf("%d ",arr[i]);
    }
    printf("After swapping\n");
    for(int j=0;j<2;j++)
    {
    for(i=0;i<n-1;i++)
    {
        if(i==0)
        {
            temp=arr[i];

        }
        
        arr[i]=arr[i+1];
        
    }
    arr[n-1]=temp;
    }
    for(i=0;i<n;i++)
    {
        printf("%d ",arr[i]);
    }
}    
